{
  "name": "error-clicks",
  "private": true,
  "scripts": {
    "start": "hs .",
    "build": "tsc && terser ./dist/index.js -o ./dist/index.js && yarn empty-line && yarn concat && yarn iife",
    "concat": "cat ./main.js >> ./dist/index.js",
    "empty-line": "echo '\n' >> ./dist/index.js",
    "iife": "echo \"(function (){\n$(cat ./dist/index.js)\n})()\" > ./dist/index.js"
  },
  "devDependencies": {
    "http-server": "^0.12.3",
    "shared-dev": "workspace:*",
    "terser": "^5.3.8",
    "typescript": "^4.0.3"
  }
}
